{
    "name": "seongbae/canvas7",
    "description": "A Laravel 7 admin panel package.",
    "homepage": "https://github.com/seongbae/canvas",
    "license": "MIT",
    "authors": [
        {
            "name": "Seong Bae",
            "email": "seong@lnidigital.com",
            "role": "Developer"
        }
    ],
    "require": {
        "appstract/laravel-options": "^4.0",
        "barryvdh/laravel-snappy": "^0.4.7",
        "laravel/cashier": "^10.7",
        "rap2hpoutre/laravel-log-viewer": "^1.3",
        "spatie/laravel-activitylog": "^3.11",
        "spatie/laravel-flash": "^1.3",
        "spatie/laravel-medialibrary": "^7.0.0",
        "spatie/laravel-permission": "^3.11",
        "spatie/laravel-searchable": "^1.6",
        "spatie/laravel-tags": "^2.6",
        "stripe/stripe-php": "^7.23",
        "yajra/laravel-datatables": "1.5",
        "yajra/laravel-datatables-oracle": "~9.0",
        "intervention/image": "^2.4@dev"
    },
    "autoload": {
        "psr-4": {
            "Seongbae\\Canvas\\": "src",
            "Spatie\\Flash\\": "vendor/spatie/laravel-flash/src"
        },
        "files": [
            "src/Helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Seongbae\\Canvas\\CanvasServiceProvider",
                "Appstract\\Options\\OptionsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}
